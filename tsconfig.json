{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "jsx": "react",
        "jsxFactory": "ExF",
        "sourceMap": true,
        "downlevelIteration": true,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "forceConsistentCasingInFileNames": true
    }
}